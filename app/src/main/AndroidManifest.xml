<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pemilu.com.pemiluosisversion1_0">

    <uses-sdk
        android:maxSdkVersion="23"
        android:minSdkVersion="14" />

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <android:uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:name="android.support.multidex.MultiDexApplication"
        android:allowBackup="true"
        android:icon="@drawable/ikon"
        android:label="NEW ELECTION 2017"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Splashscreen"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Wizard"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" />
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Pemilihan"
            android:screenOrientation="portrait" />
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Koneksi"
            android:screenOrientation="portrait" />
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Kandidat1"
            android:screenOrientation="portrait" />
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Kandidat2"
            android:screenOrientation="portrait" />
        <activity
            android:name="pemilu.com.pemiluosisversion1_0.Kandidat3"
            android:screenOrientation="portrait" />
        <activity android:name="pemilu.com.pemiluosisversion1_0.Berhasil_pilih"></activity>
    </application>

</manifest>